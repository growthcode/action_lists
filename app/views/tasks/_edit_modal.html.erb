<div class="modal" id="edit_task">
  <div class="modal-dialog modal-lg">
    <div class="modal-content">
      <div class="modal-header">
        <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">Ã—</span></button>
        <h4 class="modal-title"><%= @task.deed %></h4>
      </div>
      <div class="modal-body">
        <p>description: <%= @task.description %></p>
        <p>role: <%= @task.role %></p>
        <p>person: <%= @task.person %></p>
        <p>priority: <%= @task.priority %></p>
        <p>position: <%= @task.position %></p>
        <p>included: <%= @task.included %></p>
        <p>inbox: <%= @task.inbox %></p>
        <p>minutes: <%= @task.minutes %></p>
        <p>plan_id: <%= @task.plan_id %></p>
      </div>
      <div class="modal-footer">
        <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
        <button type="button" class="btn btn-primary">Save changes</button>
      </div>
    </div><!-- /.modal-content -->
  </div><!-- /.modal-dialog -->
</div>
